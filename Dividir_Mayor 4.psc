Algoritmo Dividir_Mayor
	// Definir variables
	Definir Num1, Num2 Como Entero;
	Definir R Como Real;
	// Entrada de Datos
	Escribir "Intrudusca los siguientes valores: ";
	Escribir "Valor 1: ";
	Leer Num1;
	Escribir "Valor2: ";
	Leer Num2;
	// Operación
	si Num1>Num2 Entonces
		R<-Num1/Num2;
	SiNo
		R<-Num2/Num1
	FinSi
	// Salida de datos
	Escribir "El resultado es: ",R;
	
	
	
FinAlgoritmo


